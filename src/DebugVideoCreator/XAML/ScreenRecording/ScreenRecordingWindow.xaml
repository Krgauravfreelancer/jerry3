        <screenrecorder_usercontrol:ScreenRecorder_Control Name="Recorder" Loaded="Recorder_Loaded" CloseWindow="Recorder_CloseWindow" DeleteMedia="Recorder_DeleteMedia" MediaRecordingCompleted="ScreenRecorder_Control_SaveClicked2"/>
