<UserControl x:Class="Timeline.UserControls.Controls.TrackControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Timeline.UserControls.Controls"
             xmlns:config="clr-namespace:Timeline.UserControls.Config"
             xmlns:converters="clr-namespace:Timeline.UserControls.Converters"
             xmlns:models="clr-namespace:Timeline.UserControls.Models"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="800">
    <UserControl.Resources>
        <converters:MultiplyConverter x:Key="MultiplyConverter"/>
    </UserControl.Resources>
    <Border BorderThickness="0, 0.5, 0.5,0.5" BorderBrush="LightSlateGray" >
          
        <Grid>
            <Rectangle x:Name="TrackControlBackground" Fill="LightBlue" Height="20"  Stretch="Fill" StrokeThickness="0" Width="Auto"/>

            <Canvas x:Name="EventsCanvas" Height="30" Background="Transparent" >
            </Canvas>
        </Grid>


    </Border>




</UserControl>
